<html>
<head>
    {% block meta %}
        <meta charset="utf-8">
        <meta name="viewport" content="height=device-height,width=device-width,initial-scale=1">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <title>Services</title>
    {% endblock %}

    {% block stylesheets %}
        {{ head|raw }}
    {% endblock %}

</head>
<body>
{% block body %}
    {% block header %}
        {% include "layout/header.html.twig" %}
    {% endblock %}

    {% block nav %}
        {% include "layout/nav.html.twig" %}
    {% endblock %}

    {% block services %}
    <div class="services-container">
        <div class="row column">
            <div class="row services-content">
                <div class="large-5 columns services">
                    <h4>Sub Title (Generic Content)</h4>
                    <h1>Storage Solutions</h1>
                </div>
                <div class="large-7 columns services-images">
                    <h1>Images</h1>
                </div>
            </div>
        </div>
    </div>
    {% endblock %}

    {% block shareTransform %}
        {% include "layout/sharetransform.html.twig" %}
    {% endblock %}

    {% block footer %}
        {% include "layout/footer.html.twig" %}
    {% endblock %}
{% endblock %}

{% block scripts %}

{% endblock %}
</body>
</html>
